{
  "keyring.Authentication": {
    "register": {
      "method": "post",
      "path": "/authentication/register"
    },
    "getSalt": {
      "method": "get",
      "path": "/authentication/get-salt/:username"
    },
    "logIn": {
      "method": "post",
      "path": "/authentication/log-in"
    }
  },
  "keyring.Administration": {
    "acquireMailToken": {
      "method": "post",
      "path": "/administration/acquire-mail-token"
    },
    "releaseMailToken": {
      "method": "post",
      "path": "/administration/release-mail-token"
    },
    "keepAlive": {
      "method": "post",
      "path": "/administration/keep-alive"
    },
    "createKey": {
      "method": "post",
      "path": "/administration/create-key"
    },
    "readKeys": {
      "method": "get",
      "path": "/administration/read-keys"
    },
    "updateKey": {
      "method": "put",
      "path": "/administration/update-key"
    },
    "deleteKey": {
      "method": "post",
      "path": "/administration/delete-key"
    },
    "changeMasterKey": {
      "method": "post",
      "path": "/administration/change-master-key"
    },
    "changeUsername": {
      "method": "put",
      "path": "/administration/change-username"
    },
    "deleteAccount": {
      "method": "post",
      "path": "/administration/delete-account"
    },
    "getRecentSessions": {
      "method": "get",
      "path": "/administration/get-recent-sessions"
    }
  }
}
